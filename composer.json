{
    "name": "jobmetric/flyron",
    "description": "Flyron is a PHP package for asynchronous programming using Fibers and process-based concurrency, specially designed for Laravel applications.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "flyron",
        "asynchronous",
        "fiber",
        "process",
        "concurrency"
    ],
    "homepage": "https://doc.jobmetric.net/package/flyron",
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.18.6"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Flyron\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Flyron\\FlyronServiceProvider"
            ],
            "aliases": {
                "Async": "JobMetric\\Flyron\\Facades\\Async",
                "ProcessAsync": "JobMetric\\Flyron\\Facades\\ProcessAsync"
            }
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/flyron/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/flyron"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/majidmohammadian"
        }
    ]
}
